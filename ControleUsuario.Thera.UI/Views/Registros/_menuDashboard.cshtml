<!--topo-->
<div class="col-md-12 mt-4">
    <div class="d-flex flex-row">
        <div class="col-3 logo-dash">
            <img src="@Url.Content("~/Content/Images/logo.svg")" alt="Logo" />
        </div>
        <div class="col-7 mt-3">
            <span class="span-white">Olá, @Model.Usuario</span>
        </div>
        <div class="col-3 mb-3 d-flex justify-content-end align-items-end">
            <button id="close" class="btn btn-closeDash btn-lg"></button>
        </div>
    </div>
</div>
<!--horario-->
<div class="col-md-8 mt-4 mx-auto">
    <div class="d-flex flex-row">
        <div class="col-3 mx-3">
            <span class="span-white" id="current-date"></span>
        </div>
        <div class="mx-1 col-3">
            <span class="span-white" id="current-time"></span>
        </div>
        <div class="col-5">
            @*<span class="span-yellow" id="current-user">Tempo 07:15:47</span>*@
        </div>
    </div>
</div>