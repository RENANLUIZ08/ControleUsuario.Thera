@model  int
@{
    var eTimeSheets = Enum.GetValues(typeof(Domain.Thera.Enum.ETimeSheet))
                .Cast<Domain.Thera.Enum.ETimeSheet>().ToList();
}

<div class="col-md-9 mx-auto">
    <div class="mt-3 d-flex flex-row">
        @foreach (var etimeSheet in eTimeSheets)
        {
            <div class="btn-acao">
                <button id="@((int)etimeSheet)" onclick="SendTimeSheet(@Model, @(etimeSheet.GetHashCode()), '@(Domain.Thera.Enum.EHelper.GetDescription(etimeSheet))');" class="btn btn-default btn-lg">@(Domain.Thera.Enum.EHelper.GetDescription(etimeSheet))</button>
            </div>
        }

    </div>
</div>